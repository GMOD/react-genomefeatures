{
  "name": "react-genomefeatures",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "GMOD/react-genomefeatures",
  "type": "module",
  "description": "A React+D3 based genome feature rendering",
  "keywords": [
    "genome feature viewer",
    "genomics",
    "genes",
    "transcripts"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/react-genomefeatures.umd.cjs",
  "module": "./dist/react-genomefeatures.js",
  "types": "./dist/react-genomefeatures.d.ts",
  "exports": {
    ".": {
      "types": "./dist/react-genomefeatures.d.ts",
      "import": "./dist/react-genomefeatures.js",
      "require": "./dist/react-genomefeatures.umd.cjs"
    },
    "./style.css": "./dist/react-genomefeatures.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build-demo": "yarn build -c vite-gh-pages.config.ts",
    "preversion": "npm run lint && npm run build",
    "postversion": "git push --follow-tags",
    "predeploy": "yarn build-demo",
    "deploy": "gh-pages -d out --nojekyll",
    "postdeploy": "git push --follow-tags"
  },
  "dependencies": {
    "genomefeatures": "^1.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "prettier": "^3.5.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0"
  }
}
